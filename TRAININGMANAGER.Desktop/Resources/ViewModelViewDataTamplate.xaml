<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controlPanelViewModels="clr-namespace:TRAININGMANAGER.Desktop.ViewModels.ControlPanel"
                    xmlns:controlPanelView="clr-namespace:TRAININGMANAGER.Desktop.Views.ControlPanel"
                    xmlns:usersViewModels="clr-namespace:TRAININGMANAGER.Desktop.ViewModels.Users"
                    xmlns:usersView="clr-namespace:TRAININGMANAGER.Desktop.Views.Users">
                    
    <DataTemplate DataType="{x:Type controlPanelViewModels:ControlPanelViewModel}">
        <controlPanelView:ControlPanelView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:UsersViewModel}">
        <usersView:UsersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:PlayersViewModel}">
        <usersView:PlayersView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type usersViewModels:TrainersViewModel}">
        <usersView:Trainers/>
    </DataTemplate>
</ResourceDictionary>